    <script src="~/Content/assets/js/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- For Demo Sparkline -->
    <script src="~/Content/assets/js/plugins/sparkline/jquery.sparkline.demo.js"></script>

    <!-- Angular JS -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.0-beta.14/angular.min.js"></script>
    <!-- ToDo List Plugin -->
    <script src="~/Content/assets/js/angular/todo.js"></script>



    <!-- Calendar JS -->
    <script src="~/Content/assets/js/plugins/calendar/calendar.js"></script>
    <!-- Calendar Conf -->
    <script src="~/Content/assets/js/plugins/calendar/calendar-conf.js"></script>



    <!-- Custom JQuery -->
    <script src="~/Content/assets/js/app/custom.js" type="text/javascript"></script>
@{
    int[] thisyear = (int[])Session["thisyear"];
    int[] lastyear = (int[])Session["last"];
    object[] multi = (object[])Session["multi"];
    int[] catvals = (int[])Session["catvals"];
    string[] catnames = (string[])Session["catnames"];
    int counter = 0;
    
}
    <script >

    $("#area-chart").dxChart({
        dataSource: [

{ country: "January", y014: @thisyear[0], y65 : @lastyear[0] },
    { country: "February", y014: @thisyear[1], y65 : @lastyear[1] },
    { country: "March", y014: @thisyear[2], y65 : @lastyear[2] },
    { country: "April", y014: @thisyear[3], y65 : @lastyear[3] },
    { country: "May", y014: @thisyear[4], y65 : @lastyear[4] },
    { country: "June", y014: @thisyear[5], y65 : @lastyear[5] },
    { country: "July", y014: @thisyear[6], y65 : @lastyear[6] },
    { country: "August", y014: @thisyear[7], y65 : @lastyear[7] },
    { country: "September", y014: @thisyear[8], y65 : @lastyear[8] },
    { country: "October", y014: @thisyear[9], y65 : @lastyear[9] },
    { country: "November", y014: @thisyear[10], y65 : @lastyear[10] },
    { country: "December", y014: @thisyear[11], y65 : @lastyear[11] }
        ],
        commonSeriesSettings: {
            type: "area",
            argumentField: "country"
        },
        series: [
        
        { valueField: "y1564", name: "Before 2014", color: "#fbcf04" },
        { valueField: "y014", name: "2015", color: "#7a34c3" },
        { valueField: "y65", name: "2014", color: "#6ab704" }
        ],
        argumentAxis:{
            valueMarginsEnabled: false
        },
        legend: {
            visible: true
        },
        valueAxis:{
            grid:{
                color: '#9D9EA5',
                width: 0.1
            }
        },
    });




    $("#bar-chart").dxChart({
        dataSource: [
            @foreach(var n in catnames)
            {
            @: {day: "@n" , oranges : @catvals[counter] },
            counter++;
            }
        ],
        valueAxis:{
            grid:{
                color: '#5EFEA5',
                width: 0.1
            }
        },
        legend: {
            visible: true,
        },
        series: {
            argumentField: "day",
            valueField: "oranges",
            name: "Ideas",
            visible: false,
            type: "bar",
            color: '#66CCFF'
        }
    });



</script>



