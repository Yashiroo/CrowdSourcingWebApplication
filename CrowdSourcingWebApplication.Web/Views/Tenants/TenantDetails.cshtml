@model CrowdSourcingWebApplication.Domain.Entities.Tenant
@using CrowdSourcingWebApplication.Domain.Entities;
@{
    Layout = "";
    Tenant tenant = (Tenant)Session["tenant"];
}
<!DOCTYPE html>
<html lang="en">
<use master="EmptyMaster" />
<head>
    <title>Tenant Control Panel - My Information</title>
    @{
        ViewBag.ExcludeMenu = true;
    }
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">



    <meta name="description" content="">
    <meta name="author" content="Akshay Kumar">

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="~/Content/assets/css/bootstrap/bootstrap.css" />

    <!-- Calendar Styling  -->
    <link rel="stylesheet" href="~/Content/assets/css/plugins/calendar/calendar.css" />

    <!-- Fonts  -->
    <link href='http://fonts.googleapis.com/css?family=Raleway:400,500,600,700,300' rel='stylesheet' type='text/css'>

    <!-- Base Styling  -->
    <link rel="stylesheet" href="~/Content/assets/css/app/app.v1.css" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body data-ng-app>


    <!-- Preloader -->
    <div class="loading-container">
        <div class="loading">
            <div class="l1">
                <div></div>
            </div>
            <div class="l2">
                <div></div>
            </div>
            <div class="l3">
                <div></div>
            </div>
            <div class="l4">
                <div></div>
            </div>
        </div>
    </div>
    <!-- Preloader -->


    <aside class="left-panel">

        <div class="user text-center">
            <img src="~/Content/assets/images/avtar/tenant.jpg" class="img-circle" alt="...">
            <h4 class="user-name">@Model.Username</h4>

            <div class="dropdown user-login">

            </div>
        </div>



        <nav class="navigation">
            <ul class="list-unstyled">
                <li ><a href=@Url.Action("Index")><i class="fa fa-bookmark-o"></i><span class="nav-label">Dashboard</span></a></li>
                <li class="active has-submenu">
                    <a href=@Url.Action("#")><i class="fa fa-bookmark-o"></i><span class="nav-label">Tenant</span></a>
                    <ul class="list-unstyled">
                        <li><a href=@Url.Action("TenantDetails")>My Information</a></li>
                        <li><a href=@Url.Action("Index","IdeaReviewer")>Idea Reviewers</a></li>
                    </ul>
                </li>
                <li>
                    <a href=@Url.Action("Templates")><i class="fa fa-comment-o"></i> <span class="nav-label">Templates</span></a>
                </li>
                <li class="has-submenu">
                    <a href="#"><i class="fa fa-flag-o"></i> <span class="nav-label">Categories</span></a>
                    <ul class="list-unstyled">
                        <li><a href=@Url.Action("AddCategory")>Add Category</a></li>
                        <li><a href=@Url.Action("Categories")>Manage Categories</a></li>
                    </ul>
                </li>
                <li>
                    <a href=@Url.Action("Statistics")><i class="fa fa-file-text-o"></i> <span class="nav-label">Statistics</span></a>
                </li>
                <li>
                    <a href="#"><i class="fa fa-heart-o"></i> <span class="nav-label">Logout</span></a>
                </li>


            </ul>
        </nav>

    </aside>
    <!-- Aside Ends-->

    <section class="content">

        <header class="top-head container-fluid">
            <button type="button" class="navbar-toggle pull-left">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>



            <nav class=" navbar-default hidden-xs" role="navigation">

            </nav>

            <ul class="nav-toolbar">

                <li class="dropdown">
                    <a href="#" data-toggle="dropdown"><i class="fa fa-ellipsis-v"></i></a>
                    <div class="dropdown-menu lg pull-right arrow panel panel-default arrow-top-right">
                        <div class="panel-heading">
                            More Apps
                        </div>
                        <div class="panel-body text-center">
                            <div class="row">
                                <div class="col-xs-6 col-sm-4"><a href="#" class="text-green"><span class="h2"><i class="fa fa-envelope-o"></i></span><p class="text-gray no-margn">Messages</p></a></div>
                                <div class="col-xs-6 col-sm-4"><a href="#" class="text-purple"><span class="h2"><i class="fa fa-calendar-o"></i></span><p class="text-gray no-margn">Events</p></a></div>

                                <div class="col-xs-12 visible-xs-block"><hr></div>

                                <div class="col-xs-6 col-sm-4"><a href="#" class="text-red"><span class="h2"><i class="fa fa-comments-o"></i></span><p class="text-gray no-margn">Chatting</p></a></div>

                                <div class="col-lg-12 col-md-12 col-sm-12  hidden-xs"><hr></div>

                                <div class="col-xs-6 col-sm-4"><a href="#" class="text-yellow"><span class="h2"><i class="fa fa-folder-open-o"></i></span><p class="text-gray">Folders</p></a></div>

                                <div class="col-xs-12 visible-xs-block"><hr></div>

                                <div class="col-xs-6 col-sm-4"><a href="#" class="text-primary"><span class="h2"><i class="fa fa-flag-o"></i></span><p class="text-gray">Task</p></a></div>
                                <div class="col-xs-6 col-sm-4"><a href="#" class="text-info"><span class="h2"><i class="fa fa-star-o"></i></span><p class="text-gray">Favorites</p></a></div>
                            </div>
                        </div>
                    </div>
                </li>
            </ul>
        </header>
        <!-- Header Ends -->


        <div class="warper container-fluid">

            <div class="page-header"><h1>Control Panel <small></small></h1></div>


            <div class="row">



            </div>
            <!-- AREA TO MODIFY -->
            @using CrowdSourcingWebApplication.Domain.Entities;
            @{
                bool? isNew = Convert.ToBoolean(TempData["isNew"]);
                Tenant p = (Tenant)Session["Tenant"];
            }

            <div class="page-header"><h3> <small> </small></h3></div>

            <div class="row">

                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">Tenant Information</div>
                        <div class="panel-body">

                            <form class="form-horizontal" role="form">


                                <hr class="dotted">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Username</label>
                                    <div class="col-sm-7">
                                        <p class="form-control-static">@Html.DisplayFor(model => p.Username)</p>
                                    </div>
                                </div>
                                <hr class="dotted">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Email</label>
                                    <div class="col-sm-7">
                                        <p class="form-control-static">@Html.DisplayFor(model => p.Email)</p>
                                    </div>
                                </div>
                                <hr class="dotted">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Password</label>
                                    <div class="col-sm-7">
                                        <p class="form-control-static">@Html.DisplayFor(model => p.Password)</p>
                                    </div>
                                </div>


                                <hr class="dotted">
                                <div class="form-group">
                                    <label class="col-sm-2 control-label">Country</label>
                                    <div class="col-sm-7">
                                        <p class="form-control-static">@Html.DisplayFor(model => p.Country)</p>
                                    </div>
                                </div>



                            </form>
                        </div>
                    </div>
                </div>

            </div>






            <!-- END OF AREA TO MODIFY -->
        </div>





        <!-- Warper Ends Here (working area) -->
    </section>

        <footer class="container-fluid footer">
            <a href="#" class="pull-right scrollToTop"><i class="fa fa-chevron-up"></i></a>
        </footer>


    
    <!-- Content Block Ends Here (right box)-->
    <!-- JQuery v1.9.1 -->
    <script src="~/Content/assets/js/jquery/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="~/Content/assets/js/plugins/underscore/underscore-min.js"></script>
    <!-- Bootstrap -->
    <script src="~/Content/assets/js/bootstrap/bootstrap.min.js"></script>

    <!-- Globalize -->
    <script src="~/Content/assets/js/globalize/globalize.min.js"></script>

    <!-- NanoScroll -->
    <script src="~/Content/assets/js/plugins/nicescroll/jquery.nicescroll.min.js"></script>

    <!-- Chart JS -->
    <script src="~/Content/assets/js/plugins/DevExpressChartJS/dx.chartjs.js"></script>
    <script src="~/Content/assets/js/plugins/DevExpressChartJS/world.js"></script>
    <!-- For Demo Charts -->
    <script src="~/Content/assets/js/plugins/DevExpressChartJS/demo-charts.js"></script>
    <script src="~/Content/assets/js/plugins/DevExpressChartJS/demo-vectorMap.js"></script>

    <!-- Sparkline JS -->
    <script src="~/Content/assets/js/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!-- For Demo Sparkline -->
    <script src="~/Content/assets/js/plugins/sparkline/jquery.sparkline.demo.js"></script>

    <!-- Angular JS -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.0-beta.14/angular.min.js"></script>
    <!-- ToDo List Plugin -->
    <script src="~/Content/assets/js/angular/todo.js"></script>



    <!-- Calendar JS -->
    <script src="~/Content/assets/js/plugins/calendar/calendar.js"></script>
    <!-- Calendar Conf -->
    <script src="~/Content/assets/js/plugins/calendar/calendar-conf.js"></script>



    <!-- Custom JQuery -->
    <script src="~/Content/assets/js/app/custom.js" type="text/javascript"></script>



