    <meta name="description" content="">
    <meta name="author" content="Akshay Kumar">

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="~/Content/assets/css/bootstrap/bootstrap.css" />

    <!-- Calendar Styling  -->
