</head>
<body data-ng-app>



        <div class="header" id="home">
            <div class="header_top">
                <div class="wrap">
                    <div class="logo">
                        <a href="index.html"><img src="~/Images/logo.png" alt="" /></a>
                    </div>
                    <div class="menu">
                        <ul>
                            <li class="current"><a href="#section-1" class="scroll"> Compare Hosting Providers</a></li>
                            <li><a href="#section-2" class="scroll">About</a></li>
                            <li><a href="#section-3" class="scroll">How it works</a></li>
                            <li><a href="#section-4" class="scroll">Contact</a></li>

                        </ul>
                    </div>
                    <div class="clear"></div>
                </div>
            </div>
        </div>
        <!-- Header Ends -->


        <div class="warper container-fluid">

            <div class="page-header"><h1>title here <small></small></h1></div>

            <!-- AREA TO MODIFY -->
            <center>
                <div class="row" style="width:700px ; position : center">

                    <div class="panel panel-default">
                        <div class="panel-heading">Registration Form</div>
                        <div class="panel-body">


                            <form method="post" class="validator-form" action="/Subscribe/aaaaa">

                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-lg-6">
                                            <label class="control-label">First name</label>
                                            @Html.EditorFor(model => model.givenName, new { htmlAttributes = new { @class = "form-control form-control-flat " } })
                                        </div>
                                        <div class="col-lg-6">
                                            <label class="control-label">Last name</label>
                                            @Html.EditorFor(model => model.surname, new { htmlAttributes = new { @class = "form-control form-control-flat " } })
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="control-label">Username</label>
                                    @Html.EditorFor(model => model.displayName, new { htmlAttributes = new { @class = "form-control form-control-flat ", @name = "email" } })
                                </div>

                                <div class="form-group">
                                    <label class="control-label">Domain Name</label>
                                        @Html.EditorFor(model => model.userPrincipalName, new { htmlAttributes = new { @class = "form-control form-control-flat ", @name = "email" } })
                                    <span>@@invoker.onmicrosoft.com</span>
                                    <br />
                                    <label class="control-label">Email Address</label>
                                    @Html.EditorFor(model => model.mailNickname, new { htmlAttributes = new { @class = "form-control form-control-flat ", @name = "email" } })

                                    <label class="control-label">Password</label>
                                    @Html.EditorFor(model => model.passwordProfile.password, new { htmlAttributes = new { @class = "form-control form-control-flat ", @type="password" } })
                                </div>


                                <hr class="dotted">

                                <div class="form-group">
                                    <button type="submit" class="btn btn-primary" name="signup" value="Validate">Validate</button>
                                    <button type="button" class="btn btn-info" id="resetBtn">Reset form</button>
                                </div>
                            </form>


                        </div>
                    </div>



                    <div class="page-header"><h3> <small> </small></h3></div>

                    <div class="row">

                        <div class="col-md-12">

                        </div>

                    </div>






                    <!-- END OF AREA TO MODIFY -->




                </div>

            </center>



        <!-- Warper Ends Here (working area) -->

    <footer class="container-fluid footer">
        <a href="#" class="pull-right scrollToTop"><i class="fa fa-chevron-up"></i></a>
    </footer>



    <!-- Content Block Ends Here (right box)-->
    <!-- JQuery v1.9.1 -->
    <script src="~/Content/assets/js/jquery/jquery-1.9.1.min.js" type="text/javascript"></script>
    <script src="~/Content/assets/js/plugins/underscore/underscore-min.js"></script>
    <!-- Bootstrap -->
