    <script src="~/Content/assets/js/plugins/DevExpressChartJS/world.js"></script>
    <!-- For Demo Charts -->
    <script src="~/Content/assets/js/plugins/DevExpressChartJS/demo-charts.js"></script>
    <script src="~/Content/assets/js/plugins/DevExpressChartJS/demo-vectorMap.js"></script>

